<Window x:Class="XML_DB.LaunchSqlCommand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LaunchSqlCommand" Height="400" Width="600">
    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="{DynamicResource MainColor}"/>
        </Grid.Background>
        <TextBox x:Name="textBoxCommand" Height="121" Margin="10,10,10,0" TextWrapping="Wrap" Text="Kwerenda ( SQL )" VerticalAlignment="Top" BorderThickness="2"/>
        <TextBox x:Name="textBoxReturn" Margin="10,193,10,10" TextWrapping="Wrap" Text="Wynik polecenia" BorderThickness="2"/>
        <Button x:Name="buttonLaunchSql" Content="Wykonaj" Margin="231,145,231,0" Height="30" VerticalAlignment="Top" Click="buttonLaunchSql_Click" BorderThickness="2"/>

    </Grid>
</Window>
